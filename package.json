{
  "name": "list-config-plugins",
  "version": "1.1.8",
  "description": "find available expo config plugins and see if they are used",
  "author": {
    "name": "WookieFPV (Lukas MÃ¼ller)",
    "url": "github.com/WookieFPV"
  },
  "repository": {
    "type": "git",
